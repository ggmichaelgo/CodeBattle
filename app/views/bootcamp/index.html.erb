<%= stylesheet_link_tag("bootcamp/index") %>
<%= javascript_include_tag('bootcamp/index')%>
<br>

<%
	# list = []
	# (1...5).each do |i|
	# 	list << IOQuestion.new(:name => i.to_s)
	# end	
	# list.reverse!
	list = @list
	list.reverse!
%>

<div style='width: 622px; margin: 0 auto;'>
	<table>
		<% cnt = 0 %>
		<% while list.count > 0 %>		
		<tr class='row' id=<%= cnt+=1 %>>
			<% (0...5).each do |i| %>
				<% if list.count > 0 %>
					<% q = list.pop %>
					<td class='question_cell' onclick='location="bootcamp/solve/<%=q.category_index%>"'>
						<b class='question_number_tag'>#</b>
						<b class='question_title'><%= q.category_index %></b>
					</td>
					<td></td>
				<% end %>
			<% end %>
		</tr>
		<tr><td><br></td></tr>
		<% end %>
	</table>
</div>
